{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Genshinda",
  "version": "1.0.0",
  "identifier": "com.komedev.genshinda",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Genshinda 1.0.0",
        "width": 1280,
        "height": 720,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  }
}
